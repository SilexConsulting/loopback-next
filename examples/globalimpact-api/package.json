    "@loopback/build": "5.0.0",
    "@loopback/testlab": "2.0.2",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.4",
    "@types/lodash": "4.14.149",
    "@types/mocha": "7.0.2",
    "@types/node": "12.12.34",
    "concurrently": "5.1.0",
