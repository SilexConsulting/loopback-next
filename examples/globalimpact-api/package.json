    "@loopback/context": "3.2.0",
    "@loopback/core": "2.2.0",
    "@loopback/openapi-v3": "3.1.1",
    "@loopback/repository": "2.0.2",
    "@loopback/rest": "3.1.0",
    "@loopback/rest-explorer": "2.0.2",
    "@loopback/security": "0.2.2",
    "@loopback/service-proxy": "2.0.2",
    "@types/jsonwebtoken": "8.3.8",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-google-oauth2": "^0.1.3",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2": "^1.4.9",
    "@types/yaml": "1.2.0",
    "axios": "^0.19.2",
    "bcryptjs": "2.4.3",
    "casbin": "4.1.1",
    "debug": "4.1.1",
    "express": "4.17.1",
